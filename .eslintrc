{
  "extends": [
    "plugin:flowtype/recommended",
    "airbnb"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
    },
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "react/jsx-filename-extension": [
      1, {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/prop-types": 0,
    "react/no-did-update-set-state": 0,
    "func-names": 0,
    "no-unused-vars": [
      1, {
        "argsIgnorePattern": "res|next|^err"
      }
    ],
    "comma-dangle": 0,
    "max-len": 0,
    "no-console": 0,
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": [
      2, {
        "props": false
      }
    ],
    "arrow-parens": 0,
    "space-before-function-paren": ["error", {
        "anonymous": "never",
        "named": "ignore",
        "asyncArrow": "ignore"
    }],
    "no-return-await": 0
    }
  }
}